{
  "env": {
    "jest": true
  },
  "extends": [
    "next",
    "next/core-web-vitals",
    "next/typescript",
    "eslint:recommended"
  ],
  "globals": {
    "React": "readonly"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "no-console": "warn",
    "no-unused-vars": "warn"
  }
}
